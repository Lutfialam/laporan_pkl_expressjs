                                    tr
                                        th(width="30") no
                                        th(width="90") Nama
                                        th(width="90") jenis_kelamin
                                        th(width="90") umur
                                        th(width="90") Alamat
                                        th(width="90") photo profile
                                        th(width="110") action
                                tbody
                                    each adm in admin
                                        tr
                                            td(width="30") 1
                                            td(width="90") #{adm['nama']}
                                            td(width="90") #{adm['jenis_kelamin']}
                                            td(width="90") #{adm['umur']}
                                            td(width="90") #{adm['alamat']}
                                            td(width="90") #{adm['photoProfile']}
                                            td(width="110")
                                                div(class="row")
                                                    div(class="col-md-6")
                                                        a(class="btn btn-warning", href="/ubah") ubah
                                                    div(class="col-md-6")
                                                        a(class="btn btn-danger tombol-hapus", href="/delete_admin?id=" + adm['id']) hapus

                            button(class="btn btn-primary tombol-tambah", type='button', data-toggle='modal', data-target='#modalAdmin') tambah data
                            div(id="modalAdmin", class="modal fade bd-example-modal-lg", tabindex='-1', role='dialog', aria-labelledby='exampleModalCenterTitle', aria-hidden='true')
                                div(class="modal-dialog modal-lg modal-dialog-centered", role='document')
                                    div(class="modal-content")
                                        div(class="modal-header")
                                            h5(id="exampleModalCenterTitle", class="modal-title Modal title") Tambah Data
                                            button(class="close", type='button', data-dismiss='modal', aria-label='Close')
                                            span(aria-hidden='true') &times;
                                        div(class="modal-body")
                                            form(action="/tambahDataAdmin", method="post")
                                                div(class="row")
                                                    div(class="col-md-6")
                                                        div(class="form-group")
                                                            label(for="nama") Nama admin
                                                            input(type="text", name="nama", class="form-control", id="nama",placeholder="nama")
                                                            small(id="emailHelp" class="form-text text-muted") Isilah sesuai dengan nama admin yang ingin ditambahkan
                                                    div(class="col-md-6")
                                                        div(class="form-group")
                                                            label(for="username") username
                                                            input(type="text", name="username" ,class="form-control", id="username", placeholder="username")
                                                    div(class="col-md-6")
                                                        div(class="form-group")
                                                            label(for="password") password
                                                            input(type="password", name="password" ,class="form-control", id="password", placeholder="password")
                                                    div(class="col-md-6")
                                                        div(class="form-group")  
                                                            label(for="select") Level
                                                            select(class="form-control", name="level", id="select")
                                                                option admin
                                                    div(class="col-md-6")
                                                        div(class="form-group")
                                                            label(for="jenis_kelamin") jenis kelamin
                                                            input(type="text", name="jenis_kelamin" ,class="form-control", id="jenis_kelamin", placeholder="jenis kelamin")
                                                    div(class="col-md-6")
                                                        div(class="form-group")
                                                            label(for="umur") umur
                                                            input(type="number", name="umur" ,class="form-control", id="umur", placeholder="umur")
                                                    div(class="col-md-6")
                                                        div(class="form-group")
                                                            label(for="alamat") alamat
                                                            input(type="text", name="alamat" ,class="form-control", id="alamat", placeholder="alamat")
                                                    div(class="col-md-12")
                                                        div(class="custom-file")
                                                            input(type="file",name="photoProfile" ,class="custom-file-input", id="customFile")
                                                            label(class="custom-file-label", for="customFile") Choose file
                                                div(class="modal-footer")
                                                    button(class="btn btn-secondary", type='button', data-dismiss='modal') Close
                                                    button(class="btn btn-primary", type='submit') tambah data

